{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "DEBUG=server:* nodemon ./bin/www",
    "kill:unix": "kill `lsof -i:3000 | grep node | awk '{print $2}' | tail -1`",
    "kill:windows": "taskkill /PID `lsof -i:3000 | grep node | awk '{print $2}' | tail -1`"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "socket.io": "^4.4.1",
    "toastr": "^2.1.4"
  }
}
